{
  "name": "react-ts-router",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "api": "json-server --watch db.json --port 3001",
    "dev:all": "concurrently \"npm run dev\" \"npm run api\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "testjest": "jest",
    "testjest:watch": "jest --watch",
    "testjest:coverage": "jest --coverage",
    "testvitest": "vitest",
    "testvite": "vitest",
    "testvitest:ui": "vitest --ui",
    "testvitest:coverage": "vitest --coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "test:e2e": "start-server-and-test dev:all http://localhost:5173 cypress:run",
    "test:e2e:open": "start-server-and-test dev:all http://localhost:5173 cypress:open"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "lucide-react": "^0.503.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.5.1",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cypress": "^14.5.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jsdom": "^26.1.0",
    "json-server": "1.0.0-beta.3",
    "start-server-and-test": "^2.0.12",
    "ts-jest": "^29.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vitest": "^3.2.4"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "description": "Une application de gestion de t√¢ches construite avec React, TypeScript, React Router v7 et Tailwind CSS.",
  "main": "eslint.config.js",
  "author": "",
  "license": "ISC"
}
